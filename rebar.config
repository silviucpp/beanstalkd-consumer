{deps, [
    {erlpool, "1.4.2"},
    {ebeanstalkd, ".*", {git, "https://github.com/silviucpp/ebeanstalkd", {tag, "2.1.1"}}}
]}.

{erl_opts, [
    warnings_as_errors,
    warn_bif_clash,
    warn_obsolete_guard,
    warn_unused_import,
    warn_unused_record,
    warn_unused_vars,
    warn_unused_function,
    warn_shadow_vars,
    warn_export_vars,
    warn_export_all,
    warn_exported_vars,
    warn_deprecated_function,
    strict_validation,
    {parse_transform,lager_transform}
]}.
